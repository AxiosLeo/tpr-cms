{
  "name": "axios/tpr-cms",
  "description": "design for api development",
  "type": "project",
  "version": "3.0.0",
  "keywords": [
    "framework",
    "ORM",
    "Api",
    "CMS"
  ],
  "homepage": "https://github.com/AxiosCros/tpr-cms",
  "license": "MIT",
  "authors": [
    {
      "name": "axios",
      "email": "axioscros@aliyun.com"
    }
  ],
  "support": {
    "issues": "https://github.com/AxiosCros/tpr-cms/issues"
  },
  "require": {
    "php": ">=7.4",
    "axios/tpr-db": "^1.1",
    "axios/tpr": "^5.0",
    "minphp/session": "^1.2",
    "tmarois/filebase": "^1.0",
    "gregwar/captcha": "^1.1"
  },
  "require-dev": {
    "symfony/var-dumper": "*",
    "nette/php-generator": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "library\\": "library/",
      "api\\": "application/api/",
      "admin\\": "application/admin/",
      "blog\\": "application/blog/"
    },
    "files": [
      "library/helper.php"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "scripts": {
    "start": "./tpr build && echo http://localhost:8088 && php -S localhost:8088 -t public/",
    "fix": "php-cs-fixer fix ./"
  }
}